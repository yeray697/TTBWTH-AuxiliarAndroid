<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.phile.yrj.takethebullfighterwiththehorns">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name=".Login_Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp_Activity" />
        <activity android:name=".New_Activity"/>
        <activity android:name=".Home_Activity"
            android:theme="@style/AppThemeNoActionBar"/>
        <activity android:name=".About_Activity"/>
        <activity android:name=".Settings_Activity"/>
    </application>

</manifest>